{
  "id": null,
  "title": "URL Shortener Live Traffic + Services",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 2,
  "refresh": "5s",
  "panels": [
    {
      "type": "stat",
      "title": "Total requests / sec (all apps)",
      "gridPos": {"h": 6, "w": 6, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{job=\"app_nodes\"}[30s]))",
          "legendFormat": "all"
        }
      ]
    },
    {
      "type": "stat",
      "title": "2xx / sec",
      "gridPos": {"h": 6, "w": 4, "x": 6, "y": 0},
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{job=\"app_nodes\",status=~\"2..\"}[30s]))"
        }
      ]
    },
    {
      "type": "stat",
      "title": "3xx / sec",
      "gridPos": {"h": 6, "w": 4, "x": 10, "y": 0},
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{job=\"app_nodes\",status=~\"3..\"}[30s]))"
        }
      ]
    },
    {
      "type": "stat",
      "title": "4xx / sec",
      "gridPos": {"h": 6, "w": 4, "x": 14, "y": 0},
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{job=\"app_nodes\",status=~\"4..\"}[30s]))"
        }
      ]
    },
    {
      "type": "stat",
      "title": "5xx / sec",
      "gridPos": {"h": 6, "w": 6, "x": 18, "y": 0},
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{job=\"app_nodes\",status=~\"5..\"}[30s]))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Live requests by status code",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
      "targets": [
        {
          "expr": "sum by (status) (rate(http_requests_total{job=\"app_nodes\"}[30s]))",
          "legendFormat": "{{status}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Service usage by app node",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
      "targets": [
        {
          "expr": "sum by (instance) (rate(http_requests_total{job=\"app_nodes\"}[30s]))",
          "legendFormat": "{{instance}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency p95 (s)",
      "gridPos": {"h": 7, "w": 12, "x": 0, "y": 14},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=\"app_nodes\"}[1m])) by (le))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "ClickHouse up",
      "gridPos": {"h": 7, "w": 12, "x": 12, "y": 14},
      "targets": [
        {
          "expr": "up{job=\"clickhouse_exporter\"}",
          "legendFormat": "{{instance}}"
        }
      ]
    }
  ]
}
